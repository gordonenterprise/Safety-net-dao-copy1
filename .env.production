# Minimal environment variables for Vercel deployment
NEXTAUTH_SECRET=your-super-secret-key-change-this-in-production-vercel
NEXTAUTH_URL=https://your-app.vercel.app

# Placeholder database URL - you'll need a real one for full functionality
DATABASE_URL=postgresql://placeholder:placeholder@localhost:5432/placeholder

# Placeholder Stripe keys - add real ones for payment functionality
STRIPE_PUBLISHABLE_KEY=pk_test_placeholder
STRIPE_SECRET_KEY=sk_test_placeholder

# Feature flags for initial deployment
CLAIMS_ENABLED=false
GOVERNANCE_ENABLED=false  
MEMBERSHIP_ENABLED=false
WALLET_ENABLED=false

# Disable features that require external services for now
RATE_LIMIT_ENABLED=false
AUDIT_LOG_ENABLED=false